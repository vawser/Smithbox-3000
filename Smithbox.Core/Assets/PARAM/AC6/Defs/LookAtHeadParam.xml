<?xml version="1.0" encoding="utf-8"?>
<PARAMDEF XmlVersion="3">
  <ParamType>LOOK_AT_HEAD_PARAM_ST</ParamType>
  <DataVersion>1</DataVersion>
  <BigEndian>False</BigEndian>
  <Unicode>True</Unicode>
  <FormatVersion>203</FormatVersion>
  <Fields>
    <Field Def="f32 lookAtPerf = 1">
      <DisplayName>旋回性能（％/Sec)</DisplayName>
      <Description>LookAtの有効/無効が切り替わったときの補間性能</Description>
      <Minimum>0</Minimum>
      <Maximum>10000</Maximum>
      <Increment>0.001</Increment>
      <SortID>10</SortID>
    </Field>
    <Field Def="f32 lookAtRateV = 1">
      <DisplayName>上下補正率(0.0～1.0)</DisplayName>
      <Description>目標角度に対してどれだけ向けるか（上下）</Description>
      <Minimum>0</Minimum>
      <Maximum>1</Maximum>
      <Increment>0.001</Increment>
      <SortID>10</SortID>
    </Field>
    <Field Def="f32 lookAtRateH = 1">
      <DisplayName>左右補正率(0.0～1.0)</DisplayName>
      <Description>目標角度に対してどれだけ向けるか（左右）</Description>
      <Minimum>0</Minimum>
      <Maximum>1</Maximum>
      <Increment>0.001</Increment>
      <SortID>10</SortID>
    </Field>
    <Field Def="f32 minAngDegV = -20">
      <DisplayName>下の限界角度</DisplayName>
      <Description>下方向の限界角度</Description>
      <Minimum>-180</Minimum>
      <Maximum>180</Maximum>
      <Increment>0.1</Increment>
      <SortID>10</SortID>
    </Field>
    <Field Def="f32 maxAngDegV = 60">
      <DisplayName>上の限界角度</DisplayName>
      <Description>上方向の限界角度</Description>
      <Minimum>-180</Minimum>
      <Maximum>180</Maximum>
      <Increment>0.1</Increment>
      <SortID>10</SortID>
    </Field>
    <Field Def="f32 minAngDegH = -80">
      <DisplayName>左の限界角度</DisplayName>
      <Description>左の限界角度</Description>
      <Minimum>-180</Minimum>
      <Maximum>180</Maximum>
      <Increment>0.1</Increment>
      <SortID>10</SortID>
    </Field>
    <Field Def="f32 maxAngDegH = 80">
      <DisplayName>右の限界角度</DisplayName>
      <Description>右の限界角度</Description>
      <Minimum>-180</Minimum>
      <Maximum>180</Maximum>
      <Increment>0.1</Increment>
      <SortID>10</SortID>
    </Field>
    <Field Def="dummy8 reserved4[4]">
      <DisplayName>パディング</DisplayName>
      <DisplayFormat>%f</DisplayFormat>
      <EditFlags>Wrap</EditFlags>
      <SortID>11</SortID>
    </Field>
  </Fields>
