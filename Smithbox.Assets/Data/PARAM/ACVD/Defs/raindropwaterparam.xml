      <EditFlags>Wrap</EditFlags>
    </Field>
    <Field Def="f32 TailBlendAlpha = 1">
      <DisplayName>雨尾Alpha</DisplayName>
      <Description>尻尾の描画α</Description>
      <DisplayFormat>%.1f</DisplayFormat>
      <Minimum>0</Minimum>
      <Maximum>1</Maximum>
      <Increment>0.1</Increment>
    </Field>
    <Field Def="f32 TailOffsetTexU = 0.03">
      <DisplayName>雨尾OffsetU</DisplayName>
      <Description>尻尾描画時の歪みのUV</Description>
      <DisplayFormat>%.2f</DisplayFormat>
      <Minimum>-10</Minimum>
      <Maximum>10</Maximum>
    </Field>
    <Field Def="f32 TailOffsetTexV = 0.03">
      <DisplayName>雨尾OffsetV</DisplayName>
      <Description>尻尾描画時の歪みのUV</Description>
      <DisplayFormat>%.2f</DisplayFormat>
      <Minimum>-10</Minimum>
      <Maximum>10</Maximum>
