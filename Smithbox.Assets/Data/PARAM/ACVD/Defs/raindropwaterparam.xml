      <DisplayFormat>%.2f</DisplayFormat>
      <Minimum>0.01</Minimum>
      <Maximum>10</Maximum>
    </Field>
    <Field Def="f32 DropAutoRainTime = 0.8">
      <DisplayName>雨粒の配置時間(秒)</DisplayName>
      <Description>雨粒の配置までの時間</Description>
      <DisplayFormat>%.1f</DisplayFormat>
      <Minimum>0</Minimum>
      <Maximum>100</Maximum>
