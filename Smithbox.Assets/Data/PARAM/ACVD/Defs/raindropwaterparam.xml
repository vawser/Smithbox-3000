<?xml version="1.0" encoding="utf-8"?>
<PARAMDEF XmlVersion="3">
  <ParamType>FILTER_RAIN_DROP_WATER_PARAM_ST</ParamType>
  <DataVersion>1</DataVersion>
  <BigEndian>True</BigEndian>
  <Unicode>False</Unicode>
  <FormatVersion>103</FormatVersion>
  <Fields>
    <Field Def="f32 DropBlendAlpha = 0.7">
      <DisplayName>雨粒Alpha</DisplayName>
      <Description>雨粒単体の描画のα</Description>
      <DisplayFormat>%.1f</DisplayFormat>
      <Minimum>0</Minimum>
      <Maximum>1</Maximum>
      <Increment>0.1</Increment>
    </Field>
    <Field Def="f32 DropOffsetTexU = 0.03">
      <DisplayName>雨粒OffsetU</DisplayName>
      <Description>雨粒単体の歪みUVオフセット</Description>
      <DisplayFormat>%.2f</DisplayFormat>
      <Minimum>-10</Minimum>
      <Maximum>10</Maximum>
    </Field>
    <Field Def="f32 DropOffsetTexV = 0.03">
      <DisplayName>雨粒OffsetV</DisplayName>
      <Description>雨粒単体の歪みUVオフセット</Description>
      <DisplayFormat>%.2f</DisplayFormat>
      <Minimum>-10</Minimum>
      <Maximum>10</Maximum>
    </Field>
    <Field Def="f32 DropBaseSize = 2.5">
      <DisplayName>雨粒の基本サイズ</DisplayName>
      <Description>雨粒の基本サイズ</Description>
      <DisplayFormat>%.1f</DisplayFormat>
      <Minimum>0.1</Minimum>
      <Maximum>100</Maximum>
      <Increment>0.1</Increment>
    </Field>
    <Field Def="f32 DropRandSize = 1">
      <DisplayName>雨粒のランダムサイズ</DisplayName>
      <Description>雨粒のランダムサイズ（雨粒サイズ＝基本サイズ±ランダムサイズ）</Description>
      <DisplayFormat>%.1f</DisplayFormat>
      <Minimum>0.1</Minimum>
      <Maximum>100</Maximum>
      <Increment>0.1</Increment>
    </Field>
    <Field Def="f32 DropColorUp = 2.5">
      <DisplayName>雨粒の輝度値係数</DisplayName>
      <Description>雨粒単体の輝度値</Description>
      <DisplayFormat>%.2f</DisplayFormat>
      <Minimum>0</Minimum>
      <Maximum>10</Maximum>
    </Field>
    <Field Def="f32 DropBaseAlphaTime = 5">
      <DisplayName>雨粒消える時間(秒)</DisplayName>
      <Description>雨粒の消える時間</Description>
      <DisplayFormat>%.2f</DisplayFormat>
      <Minimum>0.01</Minimum>
      <Maximum>10</Maximum>
    </Field>
    <Field Def="f32 DropRandAlphaTime = 3">
      <DisplayName>雨粒消える時間ランダム(秒)</DisplayName>
