      <Increment>0.1</Increment>
    </Field>
    <Field Def="f32 BeforeAccelGravity">
      <DisplayName>加速前：重力加速度</DisplayName>
      <Minimum>0</Minimum>
      <Maximum>999</Maximum>
    </Field>
