<?xml version="1.0" encoding="utf-8"?>
<PARAMDEF XmlVersion="3">
  <ParamType>INTERACTIVE_SMOKE_PARAM_ST</ParamType>
  <DataVersion>1</DataVersion>
  <BigEndian>False</BigEndian>
  <Unicode>True</Unicode>
  <FormatVersion>203</FormatVersion>
  <Fields>
    <Field Def="s32 emitNum = 5">
      <DisplayName>Sfx生成数</DisplayName>
      <Description>生成する数</Description>
      <Minimum>0</Minimum>
      <Maximum>9999</Maximum>
      <SortID>5000</SortID>
    </Field>
    <Field Def="f32 lifeTime = 7">
      <DisplayName>生存時間[秒]</DisplayName>
      <Description>Sfxの生存時間</Description>
      <Minimum>0</Minimum>
      <Maximum>9999</Maximum>
      <SortID>6000</SortID>
    </Field>
    <Field Def="f32 radius = 15">
      <DisplayName>半径[m]</DisplayName>
      <Description>触れる煙の半径</Description>
      <Minimum>0</Minimum>
      <Maximum>9999</Maximum>
      <SortID>7000</SortID>
    </Field>
    <Field Def="f32 addSfxTime = 2.5">
      <DisplayName>触れるSfxの発生可能時間[秒]</DisplayName>
      <Description>追加のSfxを生成する猶予時間</Description>
      <Minimum>0</Minimum>
      <Maximum>999</Maximum>
      <SortID>8000</SortID>
    </Field>
    <Field Def="f32 offsetRandomRange = 3">
      <DisplayName>着弾点からSfxを発生させる最大距離</DisplayName>
      <Description>Sfxは着弾点から設定した距離内の水平方向の位置で発生する</Description>
      <Minimum>0</Minimum>
      <Maximum>999</Maximum>
      <SortID>7500</SortID>
    </Field>
    <Field Def="s32 sfxId = -1">
      <DisplayName>着弾点に出る煙のSfxID</DisplayName>
      <Description>触れる煙の本体となるSfxのID</Description>
      <Minimum>-1</Minimum>
      <Maximum>999999999</Maximum>
      <SortID>1000</SortID>
    </Field>
    <Field Def="s32 addMainSfxId = -1">
      <DisplayName>煙Sfxに触れた際にくっつくSFXID(コア固定)</DisplayName>
      <Description>触れた後にコアにくっつくSfxのID</Description>
      <Minimum>-1</Minimum>
      <Maximum>999999999</Maximum>
      <SortID>2000</SortID>
    </Field>
    <Field Def="s32 addSubSfxId = -1">
      <DisplayName>煙Sfxに触れた際にくっつくSFXID(パーツからランダム発生)</DisplayName>
      <Description>触れた後に体中にくっつくSfxのID</Description>
      <Minimum>-1</Minimum>
      <Maximum>999999999</Maximum>
      <SortID>2100</SortID>
    </Field>
    <Field Def="dummy8 tailPadding[32]">
      <DisplayName>末尾パディング</DisplayName>
      <Description>末尾パディング</Description>
      <SortID>99999999</SortID>
    </Field>
  </Fields>
</PARAMDEF>