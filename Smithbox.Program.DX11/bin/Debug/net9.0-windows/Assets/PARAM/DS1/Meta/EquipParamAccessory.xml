<?xml version="1.0" encoding="utf-8"?>
<PARAMMETA XmlVersion="0" >
  <Self 
  Wiki="Determines the parameters of accessories." />

  <DisplayNames>
    <NameEntry 
    Param="EquipParamAccessory" 
    Name="Rings" />
  </DisplayNames>
  
  <Field>
    <refId 
    AltName="Call ID" 
    Wiki="Reference ID to use for this accessory." 
    Refs="SpEffectParam(refCategory=2),Bullet(refCategory=1),BehaviorParam_Pc(refCategory=0)" />
    
    <sfxVariationId 
    AltName="Particle Variation ID" 
    Wiki="Specify SFX variation (used to identify SFX in combination with TimeActEditor ID)" />

    <weight 
    AltName="Weight" 
    Wiki="Weight [kg]" />

    <behaviorId 
    AltName="Behavior ID" 
    Wiki="Action ID (= Skill)" 
    Refs="BehaviorParam_PC" />

    <basicPrice 
    AltName="Reinforcement Price" 
    Wiki="Soul cost of reinforcement. See ShopLineupParam for shop prices." />

    <sellValue 
    AltName="Sell Value" 
    Wiki="The price when this item is sold. Determines the souls received when feeding this weapon to Frampt" />

    <sortId 
    AltName="Sort ID" 
    Wiki="The ordering within the menu." />

    <qwcId 
    AltName="World Tendency ID" 
    Wiki="The ID this item uses for world tendency." 
    Obsolete="" />

    <equipModelId 
    AltName="Model ID" 
    Wiki="Equipment model number" />

    <iconId 
    AltName="Icon ID" 
    Wiki="Menu icon ID" />

    <shopLv 
    AltName="Shop Level" 
    Wiki="Level that can be sold at the store" />

    <trophySGradeId 
    AltName="Achievement ID" 
    Wiki="The achievement ID this part is associated with, if any." />
    
    <trophySeqId 
    AltName="Sequential Achievement ID" 
    Wiki="The sequential achievement ID this part is associated with, if any." />

    <equipModelCategory 
    AltName="Model Category" 
    Wiki="Equipment model type" 
    Enum="EQUIP_MODEL_CATEGORY" />
    
    <equipModelGender 
    AltName="Model Gender" 
    Wiki="Gender of equipment model" 
    Enum="EQUIP_MODEL_GENDER" />
    
    <accessoryCategory 
    AltName="Accessory Type" 
    Wiki="Armor category" 
    Enum="ACCESSORY_CATEGORY" />

    <refCategory 
    AltName="Call ID Type" 
    Wiki="The ID category to use for the refId field." 
    Enum="BEHAVIOR_REF_TYPE" />

    <spEffectCategory 
    AltName="SpEffect Category" 
    Wiki="The SpEffect category this spell belongs to" 
    Enum="BEHAVIOR_CATEGORY" />

    <pad
    Padding="" />

    <vagrantItemLotId 
    AltName="Vagrant: Item Lot ID" 
    Wiki="The item lot to use when this item is dropped and generates a vagrant." 
    Refs="ItemLotParam" />

    <vagrantBonusEneDropItemLotId 
    AltName="Good Vagrant: Item Lot ID" 
    Wiki="The item lot to use when this item is dropped and generates a vagrant." 
    Refs="ItemLotParam" />

    <vagrantItemEneDropItemLotId 
    AltName="Evil Vagrant: Item Lot ID" 
    Wiki="The item lot to use when this item is dropped and generates a vagrant." 
    Refs="ItemLotParam" />
    
    <isDeposit 
    AltName="Is Depositable" 
    Wiki="Can be deposited in the repository." 
    IsBool="" />
    
    <isEquipOutBrake 
    AltName="Destroy on Unequip" 
    Wiki="Is destroyed when unequipped." 
    IsBool="" />
    
    <disableMultiDropShare 
    AltName="Block Drop in Multiplayer" 
    Wiki="Block the ability to drop in multiplayer sessions." 
    IsBool="" />

    <pad1
    Padding="" />
    
  </Field>
