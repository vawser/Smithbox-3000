<?xml version="1.0" encoding="utf-8"?>
<PARAMDEF XmlVersion="3">
  <ParamType>MINI_AREA_PARAM_ST</ParamType>
  <DataVersion>1</DataVersion>
  <BigEndian>False</BigEndian>
  <Unicode>True</Unicode>
  <FormatVersion>203</FormatVersion>
  <Fields>
    <Field Def="s32 areaName = -1">
      <DisplayName>エリア名称</DisplayName>
      <Description>ミニエリアに対して割り当てられる名称のテキストID。メニューやFE、インゲーム内演出から参照される</Description>
      <EditFlags>None</EditFlags>
      <Minimum>-1</Minimum>
      <Maximum>999999999</Maximum>
      <SortID>100</SortID>
    </Field>
    <Field Def="s32 openEventFlag = -1">
      <DisplayName>開放イベントフラグ</DisplayName>
      <Description>ミニエリア毎の解放状態と紐付くイベントフラグID</Description>
      <EditFlags>None</EditFlags>
      <Minimum>-1</Minimum>
      <Maximum>999999999</Maximum>
      <SortID>200</SortID>
    </Field>
    <Field Def="s16 warningLineStartDisplayDistance = -1">
      <DisplayName>警告ライン表示開始距離(m)</DisplayName>
      <Description>エリア境界の警告ライン表示を開始するエリア境界からの距離（-1の場合警告ラインを表示しない）</Description>
      <EditFlags>None</EditFlags>
      <Minimum>-1</Minimum>
      <Maximum>9999</Maximum>
      <SortID>300</SortID>
    </Field>
    <Field Def="s16 warningLineDisplayDistance = -1">
      <DisplayName>警告ライン表示距離(m)</DisplayName>
      <Description>エリア境界の警告ラインが表示されるエリア境界からの距離（-1の場合警告ラインを表示しない）</Description>
      <EditFlags>None</EditFlags>
      <Minimum>-1</Minimum>
      <Maximum>9999</Maximum>
      <SortID>400</SortID>
    </Field>
    <Field Def="dummy8 pad_end[4]">
      <DisplayName>pad_end[4]</DisplayName>
      <SortID>401</SortID>
    </Field>
  </Fields>
</PARAMDEF>